<?xml version="1.0"?>
<configuration>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup>
  <appSettings>
    <!-- 数据库连接类型 -->
    <add key="DataProvider" value="MySQL"/>
    <!-- MySQL 连接字符串 -->
    <!--测试-->
    <add key="ConnectionString" value="server=10.0.2.31;Uid=root;Pwd=6CE3644EDD7B868B;Database=test_shove_netwin;port=3306;charset=utf8"/>

    <!--设置定时类型(100:定时周期类型; 200:每日周期类型)-->
    <add key="TimerType" value="100"/>
    
    <!--设置定时周期,单位为分钟,比如:设置20,则每隔20分钟执行一次,但不一定准时-->
    <add key="CycleTimer" value="1"/>
    
    <!--设置每日周期,格式为(小时:分钟),24小时时制 ,比如:23:05 表示每天到达23点05分之后执行-->
    <add key="DailyTimer" value="23:05"/>
    
 
    <!--高峰期开始时间-->
    <add key="PeakStartTime" value="9:00"/>
    <!--高峰期结束时间-->
    <add key="PeakEndTime" value="10:00"/>
    <!--是否启用高峰期判断(1:表示启用,0表示禁用)-->
    <add key="EnablePeak" value="1"/>
    
    
  </appSettings>
</configuration>
